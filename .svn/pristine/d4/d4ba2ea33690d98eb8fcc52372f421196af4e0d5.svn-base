            $('#toolBars').css({
                width:'1420px',
                left:'370px'
            });
            $('#mapCon').css({
                left:'320px'
            })
            $('#mapCon').html("");
            ShowMap.initMap();
        }
    })

     $('.zoomScreen').on('click',function(){
