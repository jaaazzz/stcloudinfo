body, html, div, dl, dt, dd, ul, li, p, img, label, hr {
    padding: 0;
    margin: 0;
}
html{
    overflow-x: hidden;
}
body, html {
    width: 100%;
    height: 100%;

}

body {
    font-size: 10px;
    color: #000;
    font-family: "微软雅黑";

}

label {
    display: inline;
}

/*左侧目录树菜单项的样式设置*/
.categoryList, .mapList {
    display: block;
    margin: 0;
}


ul {
    list-style: outside none none;
    margin: 0;
    padding: 0;
    margin-bottom: 0 !important;
}

img {
    border: 0;
}

/*=S =======================================主体框架 =======================================*/
#content {
    width: 100%;
    height: 100%;
    border-bottom: 1px solid #ccc;
    top: 5px;
    left: 0;
    right: 0;
    overflow: hidden;
    /* bottom: 50px;*/
}

#onlinesrc-content {
    overflow: hidden;
    width: 100%;
    height: 100%;
    position: relative;
    border-bottom: 1px solid #CCC;
    border-right: 1px solid #CCC;
}

/*=E ========================================主体框架 =====================================*/

