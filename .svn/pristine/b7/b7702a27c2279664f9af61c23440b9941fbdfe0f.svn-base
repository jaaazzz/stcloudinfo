/* 静态窗体modal begin */

.modal {
  height: auto;
  width: 530px;
  z-index: 2000;
  border: none;
  -webkit-box-shadow:none;
  box-shadow:none; }
.modal .close {
  position: absolute;
  top: 18px;
  right: 7px;
  display: inline-block;
  height: 20px;
  width: 20px;
  background: url("../images/close.png") no-repeat;
  opacity: 1;
  filter: none; 
}
.modal .close:hover {
  background: url("../images/close_hover.png") no-repeat; 
}
.modal .close-preload {
  background: url("../images/close_hover.png") no-repeat;
  display: inline;
  width: 0px;
  height: 0px;
}
.modal-backdrop {
  opacity: .3;
  filter: alpha(opacity=30); 
}
.modal-body{
  padding: 0px;
}
.modal-body .title{
  line-height: 55px;
  border-bottom: 1px solid #dcdcdc;
  padding-left: 5px;
  font-size: 14px;
}

