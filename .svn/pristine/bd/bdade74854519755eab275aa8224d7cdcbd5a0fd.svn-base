.bill-body{
  margin-top: 20px;
  min-height: 500px;
}
.content
{
    width: 1024px;
    margin: auto;
    min-height: 600px;
}
.trade-select {
  border-bottom: 1px solid #dcdcdc;
  padding-top: 20px;
  padding-bottom: 2px;
  margin-bottom: 4px;
}
.trade-field{
  line-height: 25px;
  font-size: 14px;
  color: #666666;
}
.trade-field .field{
  cursor: pointer;
  padding: 0 15px 9px 15px;
  line-height: 30px;
  font-size: 14px;
  color: #666666;
  font-family: "Microsoft YaHei", "微软雅黑";
}
.trade-field .field.select{
  color: #3F8ccd;
  font-size: 14px;
  border-bottom: 2px solid #3F8ccd;
  font-weight: bold;
}
.app-one
{
    width: 180px;
    border: 1px solid #cdcdcd;
    text-align: center;
    float: left;
    margin-top: 20px;
    background: #fff;
    margin-right: 10px;
}
.app-one-img
{
    width: 160px;
    height: 140px;
    margin: auto;
    margin-top: 10px;
    border: 1px solid #AEAEAE;
}
.app-one-img img
{
    width: 160px;
    height: 140px;
}
.app-name
{
    height: 40px;
    line-height: 40px;
    font-size: 16px;
    width: 160px;
    margin: auto;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    text-decoration: underline;
}
.app-name a
{
    color: #3F8ccd;
}
.app-opr
{
    height: 40px;
    text-align: center;
}
.app-opr a
{
    display: inline-block;
    text-align: center;
}
.app-opr .collection
{
    float: right;
    margin-right: 5px;
    line-height: 30px;
    height: 30px;
    color: #3F8ccd;
}
.app-opr .online-use
{
    height: 30px;
    border: 1px solid #3dac69;
    background-color: #3dac69;
    color: #ffffff;
    width: 100px;
    border-radius: 5px;
    line-height: 30px;
    margin-right: 13px;
    float: right;
}
.top-opr-info
{
  height: 50px;
  margin-top: 8px;
  width: 1020px;
  font-size: 14px;
}
.top-opr-info input
{
  margin-bottom: 0px;
}
.top-opr-info select
{
   height: 30px;
   outline: none;
}
.create-btn
{
  width: 130px;
  height: 30px;
  line-height: 30px;
/*  border: 1px solid #3A85C6;*/
  background-color: #3A85C6;
  color: #ffffff;
  float: right;
  text-align: center;
  border-radius: 5px;
  cursor: pointer;
  margin-top: 5px;
}
.select-btn
{
  height: 30px;
  line-height: 30px;
  display: inline-block;
  border: 1px solid #3dac69;
  background-color: #3dac69;
  color: #ffffff;
  margin-left: 20px;
  width: 60px;
  text-align: center;
  cursor: pointer;
  border-radius: 3px;
}
.itme-info
{
  width: 500px !important;
}
.modle-title
{
  height: 50px;
  line-height:50px;
  padding-left: 20px;
  font-size: 16px;
  font-weight: bold;
  border-bottom: 1px solid #cdcdcd;
}
.modle-foot
{
  height: 80px;
  line-height:50px;
  padding-left: 20px;
  font-size: 14px;
}
.ok-btn
{
  width: 130px;
  height: 30px;
  line-height: 30px;
  border: 1px solid #3dac69;
  background-color: #3dac69;
  color: #ffffff;
  float: left;
  text-align: center;
  border-radius: 5px;
  cursor: pointer;
  margin-top: 10px;
  margin-left: 60px;
}
.can-btn
{
  width: 80px;
  height: 30px;
  line-height: 30px;
  color: #3A85C6;;
  float: left;
  text-align: center;
  border-radius: 5px;
  cursor: pointer;
  margin-top: 10px;
  margin-left: 10px;
}
.table th, .table td
{
  text-align: center;
}
#DialogBySHF{
  z-index: 3000;
}
.right-side
{
    margin-left: 20px;
}
select {
  height: initial;
  width: 120px;
}
input 
{
  width: 180px;
}
#renewals-tool
{
  border: 1px solid #cdcdcd !important;
}
.table-bordered th, .table-bordered td {
   border: 0px solid #ddd; 
   text-align: left;
   height: 50px;
   line-height: 50px;
}
.table thead tr
{
  background-color: #F2F5FA;
  height: 50px;
  line-height:50px;
  padding: 0px;
  font-size: 14px;
  font-weight: normal;
}
.table thead tr th
{
  font-size: 14px;
  font-weight: normal;
  padding: 0px;
}
.dropdown .caret {
  margin-top: 23px;
  margin-left: 2px;
  border-top: 4px solid #08c;
}
.dropdown-menu
{
  margin-top: -10px;
  margin-left: -60px;
  text-align: center;
}
.text_space
{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.no-product-con
{
  width: 100%;
  margin-top: 30px;
}
.no-product-con-left
{
  width: 200px;
  text-align: left;
  float: left;
  font-size: 22px;
  padding-top: 180px;
  line-height: 30px;
  margin-left: 20px;
}
.no-product-con-right
{
  width: 550px;
  text-align:right;
  float: left;
}
.pagination ul>li>a
{
  margin-left: 3px;
}
.pagination ul>li>a, .pagination ul>li>span
{
  border-left-width: 1px;
}
.pagination ul>li>a:hover, .pagination ul>.active>a, .pagination ul>.active>span {
  background-color: #3A85C6;
  color: #fff;
  border: 1px solid #3A85C6;
}
.pagination ul>.disabled>a:hover
{
  border: 1px solid #ddd;
}
.delet-btn-no
{
  background-color:#e0e0e0;
  color:#999999;
}
.no_product_img_text
{
  position: absolute;
  z-index: 1;
  margin-top: 120px;
  margin-left: 160px;
  text-align: left;
  line-height: 30px;
}
  .container{height: 756px;}
.table-striped tbody tr:nth-child(2n) td,.table-striped tbody tr:nth-child(2n) th{background-color:#f9f9f9}
.table-striped tbody tr:nth-child(2n+1) td,.table-striped tbody tr:nth-child(2n+1) th{background-color:transparent}
#hover-trans{ box-shadow: 0 0 2px 3px #78aeda, 0 0 2px #78aeda inset;
    border-radius: 5px;
   -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-transition: all 0s;
    transition: all 0s;
    -moz-transition: all 0s;}
