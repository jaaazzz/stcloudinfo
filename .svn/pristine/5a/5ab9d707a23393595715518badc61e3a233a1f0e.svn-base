.content-top{
	height: 192px;
}
.content-top .top-menu{
	position: relative;
    top: 72px;
	height: 118px;
    border: 1px solid #fff;
    box-shadow: 0px 1px 3px rgba(0,84,176,0.1);
}
.content-top .top-menu .menu-ul{
	width: 840px;
	margin: 0 auto;
	height: 100%;
}
.content-top .top-menu ul{
	height: 100%;
}
.content-top .top-menu ul li{
	float: left;
	width: 210px;
	height: 100%;
	text-align: center;
	position: relative;
	cursor: pointer;
}
.content-top .top-menu ul li.active{
	background: #effaff;
}
.content-top .top-menu ul li .menu-li-1{
	height: 100%;
}
.content-top .top-menu ul li .menu-li-2{
	background: #effaff;
	position: absolute;
    width: 100%;
    min-height: 120px;
    display: none;
}
.content-top .top-menu ul li .menu-li-2 .item{
	float: left;
	width: 85px;
	padding: 10px;
}
.content-top .top-menu ul li .menu-li-2 .item a{
	color: #666666;
	font-size: 14px;
}
.content-top .top-menu ul li .menu-li-2 .item a:hover{
	color: #4bacdf;
}
.content-top .top-menu ul li .menu-img img{
	margin-top: 15px;
	width: 40px;
	height: 40px;
}
.content-top .top-menu ul li .menu-text{
	font-size: 16px;
	color: #333333;
	margin-top: 8px;
}
.content-middle-1 .middle-1-title,.content-middle-2 .middle-2-title,.content-middle-3 .middle-3-title{
	margin-top: 40px;
	text-align: center;
	margin-bottom: 40px;
}
.content-middle-1 .middle-1-title .title,.content-middle-2 .middle-2-title .title{
	font-size: 26px;
	font-weight: bold;
	margin: 0px 10px;
	vertical-align: middle;
}
.content-middle-1{
	margin-bottom: 56px;
}
.content-middle-1 .middle-1-list .item:first-child{
	margin-left: 0px;
}
.content-middle-1 .middle-1-list .item{
	width: 263px;
	height: 374px;
	border: 1px solid #dcdcdc;
	float: left;
	margin-left: 32px;
	text-align: center;
	position: relative;
	cursor: pointer;
}
.content-middle-1 .middle-1-list .item .item-1{
	padding: 10px;
}
.content-middle-1 .middle-1-list .item .item-2{
	position: absolute;
	width: 100%;
	height: 100%;
	top: 0px;
	background: #4bacdf;
	box-shadow: 0 0 8px 0 rgba(75,176,229,.20);
	display: none;
}
.content-middle-1 .middle-1-list .item .item-2 .item-2-list{
	width: 240px;
	margin: 0px auto;
}
.content-middle-1 .middle-1-list .item .item-2 a{
	color: #ffffff;
	font-size: 14px;
}
.content-middle-1 .middle-1-list .item .item-2 .item-2-text{
	float: left;
	width: 110px;
	margin: 15px 5px;
}
.content-middle-1 .middle-1-list .item .item-1 .item-img{
	margin-top: 15px;
}
.content-middle-1 .middle-1-list .item .item-1 .item-text-1{
	margin-top: 57px;
	font-size: 20px;
	color: #333333;
}
.content-middle-1 .middle-1-list .item .item-1 .item-text-2{
	margin-top: 24px;
	font-size: 16px;
	color: #666666;
	width: 100%;
	line-height: 1.5;
}
.container-2{
	width: 100%;
	background: #effaff;
}
.content-middle-2 .middle-2-list .middle-2-item.no-margin{
	margin-left: 0px;
}
.content-middle-2 .middle-2-list .middle-2-item{
	width: 232px;
	height: 48px;
	text-align: center;
	line-height: 48px;
	background: #ffffff;
	color: #333333;
	font-size: 14px;
	float: left;
	margin-left: 2px;
	margin-bottom: 2px;
	cursor: pointer;
}
.content-middle-2 .middle-2-list .middle-2-item.active{
	background: #4bacdf;
	color: #ffffff;
}
.content-middle-2 .middle-2-list-content{
	margin-top: 40px;
}
.content-middle-2 .middle-2-list-content .content-left{
	float: left;
}
.content-middle-2 .middle-2-list-content .content-right{
	float: right;
}
.content-middle-2 .middle-2-list-content .content-title{
	font-size: 30px;
	font-weight: bold;
	color: #333333;
}
.content-middle-2 .middle-2-list-content .content-text{
	line-height: 1.2;
	font-size: 14px;
	width: 500px;
	margin-top: 32px;
}
.content-middle-2 .middle-2-list-content .content-right img{
	width: 430px;
	height: 280px;
}
.container-3{
	width: 100%;
	background: #152935;
	height: 330px;
}
.content-middle-3 .middle-3-title .title{
	font-size: 26px;
	font-weight: bold;
	margin: 0px 10px;
	vertical-align: middle;
	color: #ffffff;
}
.point{
	vertical-align: text-top;
	margin: 10px;
}
.content-middle-3 .middle-3-list{
	width: 840px;
	margin: 0px auto;
}
.content-middle-3 .middle-3-list .middle-3-item{
	float: left;
	text-align: center;
	margin: 0px 60px;
}
.content-middle-3 .middle-3-list .middle-3-item .item-3-btn{
	width: 160px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 16px;
	color: #ffffff;
	background: #4BB0E5;
	margin-top: 24px;
}