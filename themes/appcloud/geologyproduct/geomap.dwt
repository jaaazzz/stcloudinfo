    <!--于广庆css end-->
    <!--于广庆js begin-->
    <script type="text/javascript">
        $(function(){
           var tree_data=[
               {
                   text:'基础地质图',
                   children:[
                       {text:'1：500万',img:'themes/appcloud/images/geoimgs/dd1.png'},
                       {text:'1：250万',img:'themes/appcloud/images/geoimgs/dd2.png'},
                       {text:'1：200万',img:'themes/appcloud/images/geoimgs/geologic-data.jpg'},
                       {text:'1：150万',img:'themes/appcloud/images/geoimgs/dd1.png'},
                       {text:'1：10万',img:'themes/appcloud/images/geoimgs/dd2.png'}
                   ]
               },
               {
                   text:'区域地质图',
                   children:[
                   ]
               },
               {
                   text:'矿产地质图',
                   children:[
                   ]
               },
               {
                   text:'水文地质图',
                   children:[
                   ]
               },
               {
                   text:'工程地质图',
                   children:[
                   ]
               },
               {
                   text:'环境地质图',
                   children:[
                   ]
               },
               {
                   text:'灾害地质图',
                   children:[
                   ]
               },
               {
                   text:'能源地质图',
                   children:[
                   ]
               },
               {
                   text:'海洋地质图',
                   children:[
                   ]
               },
               {
                   text:'城市地质图',
                   children:[
                   ]
               }
               ,
               {
                   text:'地球物理图',
                   children:[
                   ]
               }
           ];
            init_tree(tree_data);

        });
        function init_tree(data){
            //构造树
            var html='';
            for(var i=0;i<data.length;i++){
                var child_html='';
                for(var j=0;j<data[i].children.length;j++){
                    child_html+='<div class="child-node node" img="'+data[i].children[j].img+'">'+data[i].children[j].text+'</div>';
                }
                html+='<div class="tree-parent">'+
                        '<div class="parent"><img src="themes/appcloud/images/geoimgs/leixing.png" style="margin-right: 4px;">'+data[i].text+'</div>'+
                        '<div class="children">'+child_html+'</div>'+
                       '</div>';
            }
            $(".tree-area").html(html);
            //绑定结点事件
            $(".tree-parent .parent").click(function(){
                if($(this).parent().hasClass('open')){
                    return false;
                }
                if(!$(this).parent().find('.child-node').length){
                    return false;
                }
                $(".tree-parent.open .children").slideUp();
                $(".tree-parent.open").removeClass('open');
                $(this).parent().addClass('open');
                $(this).parent().find('.children').slideDown();
            });
            $(".child-node").click(function(){
                $(".child-node.active").removeClass('active');
                $(this).addClass('active');
                var img=$(this).attr('img');
                if(img){
                    $(".right-content-main img").attr('src',img);
                }
            });
        }
    </script>
    <!--于广庆js end-->
